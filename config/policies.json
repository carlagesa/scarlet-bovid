{
  "policies": [
    {
      "handler": {
        "export": "default",
        "module": "$import(./modules/remove-user-id)"
      },
      "name": "remove-user-id",
      "policyType": "custom-code-outbound"
    },
    {
      "handler": {
        "export": "RequestValidationInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "includeRequestInLogs": false,
          "logLevel": "info",
          "validateBody": "reject-and-log",
          "validatePathParameters": "log-only",
          "validateQueryParameters": "log-only"
        }
      },
      "name": "insert-todo-request-validation-inbound",
      "policyType": "request-validation-inbound"
    },
    {
      "handler": {
        "export": "RequestValidationInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "includeRequestInLogs": false,
          "logLevel": "info",
          "validateBody": "reject-and-log",
          "validatePathParameters": "reject-and-log",
          "validateQueryParameters": "log-only"
        }
      },
      "name": "update-todo-request-validation-inbound",
      "policyType": "request-validation-inbound"
    },
    {
      "handler": {
        "export": "RequestValidationInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "includeRequestInLogs": false,
          "logLevel": "info",
          "validateBody": "none",
          "validatePathParameters": "reject-and-log",
          "validateQueryParameters": "log-only"
        }
      },
      "name": "delete-todo-request-validation-inbound",
      "policyType": "request-validation-inbound"
    },
    {
      "handler": {
        "export": "SetHeadersInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "headers": [
            {
              "name": "my-custom-header",
              "value": "test"
            }
          ]
        }
      },
      "name": "set-headers-inbound",
      "policyType": "set-headers-inbound"
    },
    {
      "handler": {
        "export": "SetHeadersInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "headers": [
            {
              "name": "api_key",
              "value": "$env(API_KEY)",
              "overwrite": "true"
            }
          ]
        }
      },
      "name": "set-headers-inbound-1",
      "policyType": "set-headers-inbound"
    },
    {
      "handler": {
        "export": "SetHeadersInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "headers": [
            {
              "name": "api_key",
              "value": "$env(API_KEY)",
              "overwrite": true
            }
          ]
        }
      },
      "name": "set-headers-inbound-2",
      "policyType": "set-headers-inbound"
    },
    {
      "handler": {
        "export": "ApiKeyInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "allowUnauthenticatedRequests": false,
          "cacheTtlSeconds": 60
        }
      },
      "name": "api-key-inbound",
      "policyType": "api-key-inbound"
    },
    {
      "handler": {
        "export": "RateLimitInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "rateLimitBy": "ip",
          "requestsAllowed": 2,
          "timeWindowMinutes": 1
        }
      },
      "name": "rate-limit-inbound",
      "policyType": "rate-limit-inbound"
    },
    {
      "handler": {
        "export": "RateLimitInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "rateLimitBy": "function",
          "requestsAllowed": 2,
          "timeWindowMinutes": 1,
          "identifier": {
            "export": "rateLimit",
            "module": "$import(./modules/rate-limit)"
          }
        }
      },
      "name": "rate-limit-inbound-1",
      "policyType": "rate-limit-inbound"
    }
  ]
}